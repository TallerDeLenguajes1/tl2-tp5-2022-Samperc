@model List<NombreSolucion.Models.CadeteVM>

@{
    ViewData["Title"] = "MOSTRAR CADETE";
}
 


<table class="tablaCadete table ">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col"> Telefono </th>
            <th scope="col">Eliminar</th>        
            <th scope="col">Modificar</th>        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.nombre</td>
                <td>@item.Telefono</td>
                <td><button type="button" class="btn btn-danger ">@Html.ActionLink("Eliminar","eliminarCadete","Cadete",new {id=item.ID})</button></td>
                <td><button type="button" class="btn btn-danger ">@Html.ActionLink("Modidicar","modificarCadete","Cadete",new {id=item.ID})</button></td>
            </tr>
        }
    </tbody>
</table>
<button  type="button" class="btn btn-primary agreg">@Html.ActionLink("Agregar nuevo","altaCadete","Cadete" )</button>
